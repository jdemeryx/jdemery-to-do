<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jdemery_todo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="cs.ualberta.ca.jdemery_todo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="cs.ualberta.ca.jdemery_todo.ArchivedToDos"
            android:label="@string/title_activity_archived_to_dos" >
        </activity>
        <activity
            android:name="cs.ualberta.ca.jdemery_todo.AddArchiveActivity"
            android:label="@string/title_activity_add_archive" >
        </activity>
        <activity
            android:name="cs.ualberta.ca.jdemery_todo.DeleteToDoActivity"
            android:label="@string/title_activity_delete_to_do" >
        </activity>
        <activity
            android:name="cs.ualberta.ca.jdemery_todo.UnArchiveActivity"
            android:label="@string/title_activity_un_archive" >
        </activity>
        <activity
            android:name="cs.ualberta.ca.jdemery_todo.DeleteArchiveActivity"
            android:label="@string/title_activity_delete_archive" >
        </activity>
    </application>

</manifest>
